public class Problem {
	Grid2 grid;
	Grid2 solution;
	//Grid2 startState;

	public Problem(int size, Grid2 solution, Grid2 startState) {
		//grid = new Grid2(size);
		grid = startState;	// you just feed an entire grid in as the start state
		
		this.solution = solution;
		this.startState = startState;
	}

	public boolean isSolved() {
	boolean isSolved = true;
		for(i = 0; i < grid.size; i++)
			for (j = 0; j < grid.size; j++) {
				if(grid[i][j] != solution[i][j]) {
					isSolved = false;
				}
			}
		}

		return isSolved;
	}
